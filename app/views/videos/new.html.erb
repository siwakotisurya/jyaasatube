<div class="container-fluid">
	<div class="col-md-10 col-md-push-2" id="content_top">
		<h4>By Creating a Channel You Can upload Videos and Share as public or make private</h4>
		<div  id="content_top">
		<%= form_for @video, url: channel_videos_path(@channel.name),:html => {:multipart => true } do |f| %>
		<div class="col-md-4">
			<br>
			<%=flash_show%>
			<div class="alert alert-info">
  			<strong>* </strong>Enter a channel name
			</div>
			<h6>Mp4 video size up tp 6mb is only supported</h6>
			<br>
			<%= f.file_field :video%>
			<br><br>
			<button class="btn btn-primary btn-lg">Create </button>
		</div>
		<%end%>
		</div>
	</div>
		</div>